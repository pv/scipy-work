{
    <PyInit>
    Memcheck:Leak
    match-leak-kinds: possible,reachable
    ...
    fun:PyInit_*
    fun:_PyImport_LoadDynamicModuleWithSpec
    fun:_imp_create_dynamic_impl
    fun:_imp_create_dynamic
    fun:PyCFunction_Call
    fun:ext_do_call
    ...
}
{
    <PyInit>
    Memcheck:Leak
    match-leak-kinds: possible,reachable
    ...
    fun:PyInit__*
    ...
}
{
   <Py_Initialize>
   Memcheck:Leak
   match-leak-kinds: possible,reachable
   ...
   fun:Py_Initialize
   fun:Py_Main
   ...
}
{
   <GC>
   Memcheck:Leak
   match-leak-kinds: possible,reachable
   ...
   fun:_PyObject_GC_*
   ...
}
{
   <__Pyx_InitCachedConstants>
   Memcheck:Leak
   match-leak-kinds: definite,indirect
   ...
   fun:__Pyx_InitCachedConstants
   ...
}
{
   <__Pyx_AddTraceback>
   Memcheck:Leak
   match-leak-kinds: reachable
   ...
   fun:__Pyx_AddTraceback
   ...
}
{
   <__tls_get_addr>
   Memcheck:Leak
   match-leak-kinds: reachable
   ...
   fun:tls_get_addr_tail
   fun:__tls_get_addr
   ...
}
{
   <__Pyx_ExportFunction>
   match-leak-kinds: possible,reachable
   ...
   fun:__Pyx_ExportFunction
   ...
}

# Ignore anything allocated at import time
{
   <_PyImport_LoadDynamicModuleWithSpec>
   Memcheck:Leak
   match-leak-kinds: definite,possible,reachable,indirect
   ...
   fun:_PyImport_LoadDynamicModuleWithSpec
   ...
}
